<mapper namespace="Contrato">
    <insert id="registrarContrato" parameterType="map">
        INSERT INTO empenio.pago (nombre)
        VALUES(#{nombre});
    </insert>
    
    <select id="obtenerContratos" resultType="modelo.beans.Contrato">
        SELECT * FROM empenio.categorias WHERE Categorias_idCategoria = '1' ORDER BY nombre;
    </select>
</mapper>
